olumn":94},"source":"App.js","original":{"line":42,"column":49}},{"generated":{"line":87,"column":95},"source":"App.js","original":{"line":42,"column":12}},{"generated":{"line":87,"column":96},"source":"App.js","original":{"line":42,"column":6}},{"generated":{"line":87,"column":97},"source":"App.js","original":{"line":30,"column":4}},{"generated":{"line":87,"column":98},"source":"App.js","original":{"line":29,"column":2}},{"generated":{"line":88,"column":0},"source":"App.js","original":{"line":45,"column":1}},{"name":"App","generated":{"line":90,"column":15},"source":"App.js","original":{"line":47,"column":15}}],"sources":{"App.js":"import React, { useState } from \"react\"\nimport useSWR, { SWRConfig } from \"swr\";\n\nconst fetcher = (...args) => fetch(...args).then(res => res.json());\n\nconst App = () => {\n  return (\n    <SWRConfig value={{ revalidateOnFocus: false, fetcher }}>\n      <Crimes />\n    </SWRConfig>\n  );\n}\n\nfunction Crimes() {\n  const url = 'https://data.police.uk/api/crimes-street/all-crime?lat=52.629729&lng=-1.131592&date=2019-10';\n  const { data, error } = useSWR(url)\n  if (error) return <div>Error...</div>;\n  if (!data) return <div>Loading...</div>;\n\n  return <DisplayCrimes crimes={data} categories={[...new Set(data.map(crime => crime.category))]} />\n}\n\nfunction DisplayCrimes({crimes, categories}) {\n\n  const [filterCategory, setFilerCategory] = useState(null);\n\n  const filteredCrimes = filterCategory ? crimes.filter(crime => crime.category === filterCategory) : crimes\n\n  return (\n    <>\n      {categories.map(category => (\n        <button\n          onClick={() => setFilerCategory(category)}\n          key={category}\n        >\n          {category}\n        </button>\n      ))}\n\n      {filterCategory && <button onClick={() => setFilerCategory(null)}>Reset</button>}\n\n      <pre>{JSON.stringify(filteredCrimes, null, 2)}</pre>\n    </>\n  )\n}\n\nexport default App;"},"lineCount":null}},"error":null,"hash":"64ec9af9f9e0